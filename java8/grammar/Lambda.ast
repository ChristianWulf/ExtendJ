LambdaExpression : Expr ::= LambdaParameters LambdaBody;

abstract LambdaParameters;
EmptyLambdaParameter : LambdaParameters;
InferredLambdaParameter : LambdaParameters ::= First:Access Rest:Access*;
DeclaredLambdaParameter : LambdaParameters ::= First:ParameterDeclaration Rest:ParameterDeclaration*;

abstract LambdaBody;
BlockLambdaBody : LambdaBody ::= Block;
ExpressionLambdaBody : LambdaBody ::= Expr; 