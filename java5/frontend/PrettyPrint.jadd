    }
    out.print(getModifiers());
    out.print("<");
    {
      boolean first = true;
      for (PrettyPrintable p : getTypeParameterList()) {
        if (!first) {
          out.print(", ");
        }
        first = false;
        out.print(p);
      }
    }
    out.print("> ");
    out.print(getTypeAccess());
    out.print(" ");
    out.print(getID());
    out.print("(");
    {
      boolean first = true;
      for (PrettyPrintable p : getParameterList()) {
        if (!first) {
          out.print(", ");
        }
        first = false;
        out.print(p);
      }
    }
    out.print(")");
    if (hasExceptions()) {
      out.print(" throws ");
